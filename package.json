{
  "name": "nodex",
  "version": "0.1.0",
  "private": true,
  "description": "Electron + React + TypeScript + Tailwind CSS app bootstrapped with Vite.",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:renderer\" \"npm:watch:electron\" \"npm:start:electron\"",
    "dev:renderer": "vite",
    "watch:electron": "tsc -p electron/tsconfig.json --watch --preserveWatchOutput",
    "start:electron": "wait-on http://localhost:5173 dist-electron/main.js && nodemon --watch dist-electron --exec \"cross-env NODE_ENV=development VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "build": "npm run build:renderer && npm run build:electron",
    "build:renderer": "vite build",
    "build:electron": "tsc -p electron/tsconfig.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "@remixicon/react": "^4.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.5",
    "ssh2": "^1.17.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@types/ssh2": "^1.15.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^29.1.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "wait-on": "^7.2.0"
  }
}
